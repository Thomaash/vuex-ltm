{
  "name": "vuex-ltm",
  "version": "0.0.0-no-version",
  "description": "Vuex - Long Term Memory: Async modular persistence for Vuex store.",
  "keywords": [
    "Vuex",
    "persistence"
  ],
  "author": {
    "name": "Tomáš Vyčítal",
    "email": "tom.vycital@gmail.com",
    "url": "https://github.com/Thomaash"
  },
  "license": "(Apache-2.0 OR ISC)",
  "homepage": "https://github.com/Thomaash/vuex-ltm",
  "bugs": "https://github.com/Thomaash/vuex-ltm/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/Thomaash/vuex-ltm"
  },
  "main": "dist/esnext/esm.js",
  "types": "dist/types",
  "files": [
    "dist/**/*",
    "LICENSE*"
  ],
  "scripts": {
    "build": "npm run build:code && npm run build:types && npm run build:docs",
    "build:code": "rollup --config rollup.config.js",
    "build:docs": "typedoc --excludePrivate --excludeExternals --includeDeclarations --mode file --theme minimal --name 'Vuex - Long Term Memory' --out docs src && copyfiles --all --up 1 \"docs-misc/.circleci/config.yml\" \"docs\"",
    "build:types": "tsc --project tsconfig.types.json",
    "check": "es-check es5 dist/es5/*.js",
    "clean": "rimraf '{coverage,dist,docs}/{*,.*}'",
    "fix": "npm run lint -- --fix",
    "lint": "eslint --ext .js,.ts src test *.js",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "TS_NODE_PROJECT='./tsconfig.test.json' nyc mocha",
    "test-watch": "TS_NODE_PROJECT='./tsconfig.test.json' mocha --watch"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": ">0.25%"
        }
      ],
      "@babel/typescript"
    ],
    "plugins": [
      [
        "@babel/transform-runtime",
        {
          "core-js": 3,
          "regenerator": true
        }
      ],
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread"
    ]
  },
  "mocha": {
    "extension": [
      "ts"
    ],
    "timeout": 10000,
    "recursive": true,
    "require": [
      "tsconfig-paths/register",
      "ts-node/register",
      "jsdom-global/register",
      "mock-local-storage"
    ]
  },
  "nyc": {
    "all": true,
    "cache": false,
    "check-coverage": true,
    "per-file": true,
    "include": [
      "src/**/*"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts"
    ],
    "reporter": [
      "html",
      "text",
      "text-summary"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "7.11.0",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-typescript": "7.10.4",
    "@babel/runtime": "7.11.2",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/chai": "4.2.12",
    "@types/chrome": "0.0.123",
    "@types/mocha": "8.0.3",
    "@types/node": "14.11.1",
    "@types/sinon": "9.0.5",
    "@types/sinon-chai": "3.2.4",
    "@types/sinon-chrome": "2.2.9",
    "@typescript-eslint/eslint-plugin": "4.1.1",
    "@typescript-eslint/parser": "4.1.1",
    "copyfiles": "2.3.0",
    "chai": "4.2.0",
    "es-check": "5.1.0",
    "eslint": "7.9.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-mocha": "8.0.0",
    "eslint-plugin-prettier": "3.1.4",
    "gh-pages": "3.1.0",
    "husky": "4.3.0",
    "jsdom": "16.4.0",
    "jsdom-global": "3.0.2",
    "lint-staged": "10.4.0",
    "mocha": "8.1.3",
    "mock-local-storage": "1.1.15",
    "nyc": "15.1.0",
    "prettier": "2.1.2",
    "rimraf": "3.0.2",
    "rollup": "2.27.1",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.27.2",
    "sinon": "9.0.3",
    "sinon-chai": "3.5.0",
    "sinon-chrome": "3.0.1",
    "temp-dir": "2.0.0",
    "ts-node": "9.0.0",
    "tsconfig-paths": "3.9.0",
    "typedoc": "0.19.1",
    "typescript": "4.0.3",
    "vue": "2.6.12",
    "vuex": "3.5.1"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.9.6",
    "deepmerge": "^4.2.2",
    "localforage": "^1.7.3"
  },
  "peerDependencies": {
    "vue": "^2.6.11",
    "vuex": "^3.4.0"
  }
}
